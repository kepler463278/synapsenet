{
  "productName": "SynapseNet",
  "version": "0.4.0",
  "identifier": "net.synapse.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "dist"
  },
  "bundle": {
    "active": true,
    "targets": ["dmg", "msi", "deb", "appimage"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "copyright": "Copyright Â© 2024 SynapseNet Contributors",
    "category": "Productivity",
    "shortDescription": "Decentralized semantic memory network",
    "longDescription": "SynapseNet is a decentralized semantic memory network with quantum-resistant security and AI-powered knowledge management.",
    "macOS": {
      "minimumSystemVersion": "10.15"
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "app": {
    "windows": [
      {
        "title": "SynapseNet",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "updater": {
    "active": true,
    "endpoints": [
      "https://github.com/synapsenet/synapsenet/releases/latest/download/latest.json"
    ],
    "dialog": true,
